{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
      "airbnb-base",
      "plugin:codeceptjs/recommended"
  ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "linebreak-style": 0,
      "no-underscore-dangle":  "off",
      "no-extra-semi": 0
    },
    "plugins": ["jasmine", "codeceptjs"]
}
